<?xml version="1.0" encoding="UTF-8"?>
<goobiProjects>
  <project name="default">
    <createNewProcess>
      <itemlist>
        <item from="werk" multiselect="true"> Artist
          <select label="intranda EXAMPLE"> intranda GmbH EXAMPLE\, Germany </select>
        </item>
        <item from="prozess" multiselect="false"> Font type
          <select label="Antiqua">Antiqua</select>
          <select label="Gothic">Gothic</select>
          <select label="no OCR">no_ocr</select>
        </item>
        <!-- title for monograph and periodical -->
        <item from="vorlage" isnotdoctype="multivolume" required="true" ughbinding="true" docstruct="topstruct" metadata="TitleDocMain"> Titel </item>
        <item from="vorlage" isnotdoctype="multivolume" required="true" ughbinding="true" docstruct="topstruct" metadata="TitleDocMainShort"> Titel (Sorting)</item>
        <!-- title  just for the multivolume -->
        <item from="vorlage" isdoctype="multivolume" required="true" ughbinding="true" docstruct="topstruct" metadata="TitleDocMain"> Titel </item>
        <item from="vorlage" isdoctype="multivolume" required="true" ughbinding="true" docstruct="topstruct" metadata="TitleDocMainShort"> Titel (Sorting)</item>
        <!-- authors and creators-->
        <item from="vorlage" isdoctype="monograph|multivolume|periodical" ughbinding="true" docstruct="topstruct" metadata="ListOfCreators"> Authors </item>
        <!-- identifier -->
        <item from="werk" isnotdoctype="periodical" ughbinding="true" metadata="TSL_ATS" docstruct="topstruct">ATS</item>
        <item from="werk" isdoctype="periodical" ughbinding="true" metadata="TSL_ATS" docstruct="topstruct">TSL</item>
        <item from="vorlage" isdoctype="multivolume" required="true" ughbinding="true" docstruct="topstruct" metadata="CatalogIDSource"> Identifier analog c-Satz </item>
        <item from="werk" isdoctype="multivolume" required="true" ughbinding="true" docstruct="topstruct" metadata="CatalogIDDigital">Identifier digital c-Satz</item>
        <item from="vorlage" isdoctype="monograph" required="true" ughbinding="true" docstruct="topstruct" metadata="CatalogIDSource"> Identifier analog a-Satz </item>
        <item from="werk" isdoctype="monograph" required="true" ughbinding="true" docstruct="topstruct" metadata="CatalogIDDigital"> Identifier digital a-Satz</item>
        <item from="vorlage" isdoctype="periodical" required="true" ughbinding="true" docstruct="topstruct" metadata="CatalogIDSource"> Identifier analog b-Satz </item>
        <item from="werk" isdoctype="periodical" required="true" ughbinding="true" docstruct="topstruct" metadata="CatalogIDDigital"> Identifier digital b-Satz</item>
        <item from="werk" isdoctype="periodical" required="true" ughbinding="true" docstruct="topstruct" metadata="ISSN"> ISSN </item>
        <item from="vorlage" required="true" isdoctype="periodical" ughbinding="true" docstruct="firstchild" metadata="CatalogIDSource"> Identifier analog Band </item>
        <item from="werk" required="true" isdoctype="periodical" ughbinding="true" docstruct="firstchild" metadata="CatalogIDDigital"> Identifier digital Band </item>
        <!--title\, number and authors for multivolumes and periodicals -->
        <item from="vorlage" isdoctype="multivolume|periodical" required="true" ughbinding="true" docstruct="firstchild" metadata="TitleDocMain"> Titel (Volume)</item>
        <item from="vorlage" isdoctype="multivolume|periodical" required="true" ughbinding="true" docstruct="firstchild" metadata="TitleDocMainShort"> Titel (Volume) (Sorting)</item>
        <item from="vorlage" isdoctype="multivolume" ughbinding="true" docstruct="firstchild" metadata="ListOfCreators"> Authors (Volume)</item>
        <item from="vorlage" isnotdoctype="monograph" ughbinding="true" docstruct="firstchild" metadata="CurrentNo"> Volume number </item>
        <item from="vorlage" isnotdoctype="monograph" ughbinding="true" docstruct="firstchild" metadata="CurrentNoSorting"> Number (Sorting) </item>
        <item from="vorlage" isdoctype="multivolume|periodical" required="true"> Number (Benennung) </item>
        <item from="vorlage" required="true" isdoctype="multivolume" ughbinding="true" docstruct="firstchild" metadata="CatalogIDSource"> Identifier analog f-Satz </item>
        <item from="werk" required="true" isdoctype="multivolume" ughbinding="true" docstruct="firstchild" metadata="CatalogIDDigital"> Identifier digital f-Satz </item>
        <!-- other metadata for all -->
        <item from="vorlage" isdoctype="monograph|multivolume|periodical" ughbinding="true" docstruct="topstruct" metadata="PlaceOfPublication"> Place of publication </item>
        <item from="vorlage" isdoctype="monograph" ughbinding="true" docstruct="topstruct" metadata="PublicationYear"> Publication year </item>
        <item from="vorlage" isdoctype="periodical|multivolume" ughbinding="true" docstruct="firstchild" metadata="PublicationYear"> Publication year </item>
        <item from="vorlage" isdoctype="multivolume|periodical" ughbinding="true" docstruct="firstchild" metadata="PublisherName"> Publisher </item>
        <item from="vorlage" isdoctype="monograph" ughbinding="true" docstruct="topstruct" metadata="PublisherName"> Publisher </item>
        <item from="vorlage" isdoctype="periodical|multivolume" ughbinding="true" docstruct="firstchild" metadata="shelfmarksource"> Shelfmark </item>
        <item from="vorlage" isdoctype="monograph|manuscript" ughbinding="true" docstruct="topstruct" metadata="shelfmarksource"> Shelfmark </item>
        <processtitle isdoctype="multivolume">ATS+TSL+'_'+Identifier digital f-Satz+'_'+Number (Benennung)</processtitle>
        <processtitle isdoctype="monograph">ATS+TSL+'_'+Identifier digital a-Satz</processtitle>
        <processtitle isdoctype="periodical">TSL+'_'+Identifier digital b-Satz+'_'+Number (Benennung)</processtitle>
        <!-- <processtitle isnotdoctype="multivolume">ATS+TSL+'_'+PPN digital c/a-Aufnahmel+'_'+Bandnummer</processtitle> -->
        <hide>images</hide>
      </itemlist>
      <opac use="true">
        <catalogue>K10Plus</catalogue>
      </opac>
      <templates use="true"/>
      <defaultdoctype>monograph</defaultdoctype>
      <metadatageneration use="true"/>
    </createNewProcess>
    <tifheader>
      <monograph>'|[[DOC_TYPE]]'+$Doctype+'|[[MAINTITEL]]'+Titel+'|[[AUTHORS]]'+Authors+'|[[YEAR]]'+Publication year+'|[[PLACEOFPUBLICATION]]'+Place of publication+'|[[VERZ_STRCT]]'+ATS+'_'+Identifier digital a-Satz+'|'</monograph>
      <multivolume>'|[[DOC_TYPE]]'+$Doctype+'|[[HAUPTTITEL]]'+Titel+'|[[AUTOREN/HERAUSGEBER]]'+Autoren+'|[[JAHR]]'+Erscheinungsjahr+'|[[ERSCHEINUNGSORT]]'+Erscheinungsort+'|[[VERZ_STRCT]]'+ATS+'_'+PPN digital f-Satz+'_'+Nummer (Benennung)+'|'</multivolume>
      <periodical>'|[[DOC_TYPE]]'+$Doctype+'|[[HAUPTTITEL]]'+Titel+'|[[AUTOREN/HERAUSGEBER]]'+Autoren+'|[[JAHR]]'+Erscheinungsjahr+'|[[ERSCHEINUNGSORT]]'+Erscheinungsort+'|[[VERZ_STRCT]]'+TSL+'_'+PPN digital b-Satz+'_'+Nummer (Benennung)+'|'</periodical>
    </tifheader>
    <dmsImport/>
    <!--    <validate>
      <metadata startswith="PPN" docstruct="topstruct" metadata="CatalogIDDigital">PPN muss mit PPN beginnen</metadata>
      <metadata startswith="PPN" docstruct="topstruct" metadata="CatalogIDSource">PPN muss mit PPN beginnen</metadata>
      <metadata startswith="PPN" docstruct="firstchild" metadata="CatalogIDDigital">PPN muss mit PPN beginnen</metadata>
      <metadata startswith="PPN" docstruct="firstchild" metadata="CatalogIDSource">PPN muss mit PPN beginnen</metadata>
      <metadata metadata="CreatorsAllOrigin" docstruct="all" createelementfrom="Author|Editor|IllustratorArtist|Photographer|Translator|Creator">CreatorsAllOrigin automatisch anlegen</metadata>
    </validate>-->
  </project>
</goobiProjects>
