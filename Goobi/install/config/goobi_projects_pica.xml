<?xml version="1.0" encoding="UTF-8"?>
<goobiProjects>
  <project name="default">
    <createNewProcess>
      <itemlist>
        <!-- ZVDD -->
        <item docstruct="topstruct" isnotdoctype="periodical\|multivolume" metadata="_dateDigitization" multiselect="true" required="true" ughbinding="true">
                                        Datum der Digitalisierung
                                        <select label="2021"> 2021</select>
                                </item>
        <item docstruct="firstchild" isdoctype="periodical\|multivolume" metadata="_dateDigitization" multiselect="true" required="true" ughbinding="true">
                                        Datum der Digitalisierung
                                        <select label="2021"> 2021</select>
                                </item>
        <item docstruct="topstruct" isnotdoctype="periodical\|multivolume" metadata="_electronicEdition" multiselect="true" required="true" ughbinding="true">
                                        Electronic ed.
                                        <select label="[Electronic ed.]"> [Electronic ed.] </select>
                                </item>
        <item docstruct="firstchild" isdoctype="periodical\|multivolume" metadata="_electronicEdition" multiselect="true" required="true" ughbinding="true">
                                        Electronic ed.
                                        <select label="[Electronic ed.]"> [Electronic ed.]</select>
                                </item>
        <item docstruct="topstruct" isnotdoctype="periodical\|multivolume" metadata="_electronicPublisher" multiselect="true" required="true" ughbinding="true">
                                        Hersteller der digit. Ausgabe
                                        <select label="intranda EXAMPLE">intranda GmbH EXAMPLE </select>
                                </item>
        <item docstruct="firstchild" isdoctype="periodical\|multivolume" metadata="_electronicPublisher" multiselect="true" required="true" ughbinding="true">
                                        Hersteller der digit. Ausgabe
                                        <select label="intranda EXAMPLE">intranda GmbH EXAMPLE </select>
                                </item>
        <item docstruct="topstruct" isnotdoctype="periodical\|multivolume" metadata="_placeOfElectronicOrigin" multiselect="true" required="true" ughbinding="true">
                                        Herkunft der digit. Ausgabe
                                        <select label="Göttingen EXAMPLE">Göttingen EXAMPLE</select>
                                </item>
        <item docstruct="firstchild" isdoctype="periodical\|multivolume" metadata="_placeOfElectronicOrigin" multiselect="true" required="true" ughbinding="true">
                                        Herkunft der digit. Ausgabe
                                        <select label="Göttingen EXAMPLE">Göttingen EXAMPLE</select>
                                </item>
        <item docstruct="topstruct" isnotdoctype="periodical\|multivolume" metadata="_digitalOrigin" multiselect="true" required="true" ughbinding="true">
                                        Digitale Herkunft
                                        <select label="reformatted digital"> reformatted digital </select>
                                </item>
        <item docstruct="firstchild" isdoctype="periodical\|multivolume" metadata="_digitalOrigin" multiselect="true" required="true" ughbinding="true">
                                        Digitale Herkunft
                                        <select label="reformatted digital"> reformatted digital </select>
                                </item>
        <item docstruct="topstruct" isnotdoctype="periodical\|multivolume" from="vorlage" metadata="PhysicalLocation" multiselect="true" required="true" ughbinding="true">
                                        Physikalischer Standort
                                        <select label="intranda EXAMPLE">intranda GmbH Göttingen EXAMPLE </select>
                                </item>
        <item docstruct="firstchild" isdoctype="periodical\|multivolume" from="vorlage" metadata="PhysicalLocation" multiselect="true" required="true" ughbinding="true">
                                        Physikalischer Standort
                                <select label="intranda EXAMPLE">intranda GmbH Göttingen EXAMPLE </select></item>
        <item from="werk" multiselect="true">
          Artist
          <select label="intranda EXAMPLE"> intranda GmbH EXAMPLE, Germany </select>
        </item>
        <item from="prozess" multiselect="false">
          Schrifttyp
          <select label="antiqua">Antiqua</select>
          <select label="fraktur">Fraktur</select>
          <select label="kein OCR">no_ocr</select>
        </item>
        <!-- title for monograph and periodical -->
        <item from="vorlage" isnotdoctype="multivolume" required="true" ughbinding="true" docstruct="topstruct" metadata="TitleDocMain"> Titel </item>
        <item from="vorlage" isnotdoctype="multivolume" required="true" ughbinding="true" docstruct="topstruct" metadata="TitleDocMainShort"> Titel (Sortierung)
        </item>
        <!-- title just for the multivolume -->
        <item from="vorlage" isdoctype="multivolume" required="true" ughbinding="true" docstruct="topstruct" metadata="TitleDocMain"> Titel </item>
        <item from="vorlage" isdoctype="multivolume" required="true" ughbinding="true" docstruct="topstruct" metadata="TitleDocMainShort"> Titel (Sortierung)
        </item>
        <!-- authors and creators -->
        <item from="vorlage" isdoctype="monograph\|map\|multivolume\|periodical" ughbinding="false" docstruct="topstruct" metadata="ListOfCreators"> Autoren </item>
        <!-- identifier -->
        <item from="werk" isnotdoctype="periodical" ughbinding="false" metadata="TSL_ATS" docstruct="topstruct">ATS</item>
        <item from="werk" isdoctype="periodical" ughbinding="false" metadata="TSL_ATS" docstruct="topstruct">TSL</item>
        <item from="werk" isdoctype="multivolume" required="true" ughbinding="true" docstruct="topstruct" metadata="CatalogIDDigital"> PPN digital c-Satz</item>
        <item from="vorlage" isdoctype="multivolume" required="true" ughbinding="true" docstruct="topstruct" metadata="CatalogIDSource"> PPN analog c-Satz</item>
        <item from="werk" isdoctype="monograph\|map" required="true" ughbinding="true" docstruct="topstruct" metadata="CatalogIDDigital"> PPN digital a-Satz</item>
        <item from="vorlage" isdoctype="monograph\|map" required="true" ughbinding="true" docstruct="topstruct" metadata="CatalogIDSource"> PPN analog a-Satz</item>
        <item from="werk" isdoctype="periodical" required="true" ughbinding="true" docstruct="topstruct" metadata="CatalogIDDigital"> PPN digital b-Satz</item>
        <item from="vorlage" isdoctype="periodical" required="true" ughbinding="true" docstruct="topstruct" metadata="CatalogIDSource"> PPN analog b-Satz</item>
        <item from="werk" isdoctype="periodical" required="true" ughbinding="true" docstruct="topstruct" metadata="ISSN"> ISSN </item>
        <item from="werk" required="true" isdoctype="periodical" ughbinding="true" docstruct="firstchild" metadata="CatalogIDDigital"> PPN digital Band </item>
        <item from="vorlage" isdoctype="periodical" required="true" ughbinding="true" docstruct="firstchild" metadata="CatalogIDSource"> PPN analog Band</item>
        <!--title, number and authors for multivolumes and periodicals -->
        <item from="vorlage" isdoctype="multivolume\|periodical" required="true" ughbinding="true" docstruct="firstchild" metadata="TitleDocMain"> Titel (Band)</item>
        <item from="vorlage" isdoctype="multivolume\|periodical" required="true" ughbinding="true" docstruct="firstchild" metadata="TitleDocMainShort"> Titel (Band) (Sortierung)</item>
        <item from="vorlage" isdoctype="multivolume" ughbinding="false" docstruct="firstchild" metadata="ListOfCreators"> Autoren (Band)</item>
        <item from="vorlage" isnotdoctype="monograph\|map" ughbinding="true" docstruct="firstchild" metadata="CurrentNo"> Bandnummer </item>
        <item from="vorlage" isnotdoctype="monograph\|map" ughbinding="true" docstruct="firstchild" metadata="CurrentNoSorting"> Nummer (Sortierung) </item>
        <item from="vorlage" isdoctype="multivolume\|periodical" required="true"> Nummer (Benennung) </item>
        <item from="werk" required="true" isdoctype="multivolume" ughbinding="true" docstruct="firstchild" metadata="CatalogIDDigital"> PPN digital f-Satz </item>
        <item from="vorlage" isdoctype="multivolume" required="true" ughbinding="true" docstruct="firstchild" metadata="CatalogIDSource"> PPN analog f-Satz</item>
        <!-- other metadata for all -->
        <item from="vorlage" isdoctype="monograph\|map\|multivolume\|periodical" ughbinding="true" docstruct="topstruct" metadata="PlaceOfPublication"> Erscheinungsort</item>
        <item from="vorlage" isdoctype="monograph\|map" ughbinding="true" docstruct="topstruct" metadata="PublicationYear"> Erscheinungsjahr </item>
        <item from="vorlage" isdoctype="periodical\|multivolume" ughbinding="true" docstruct="firstchild" metadata="PublicationYear"> Erscheinungsjahr </item>
        <item from="vorlage" isdoctype="multivolume\|periodical" ughbinding="true" docstruct="firstchild" metadata="PublisherName"> Verlag </item>
        <item from="vorlage" isdoctype="monograph\|map" ughbinding="true" docstruct="topstruct" metadata="PublisherName"> Verlag </item>
        <item from="vorlage" isdoctype="periodical\|multivolume" ughbinding="true" docstruct="firstchild" metadata="shelfmarksource"> Signatur </item>
        <item from="vorlage" isdoctype="monograph\|map\|manuscript" ughbinding="true" docstruct="topstruct" metadata="shelfmarksource"> Signatur </item>
        <processtitle isdoctype="multivolume">ATS+TSL+'_'+PPN digital f-Satz+'_'+Nummer (Benennung)</processtitle>
        <processtitle isdoctype="monograph\|map">ATS+TSL+'_'+PPN digital a-Satz</processtitle>
        <processtitle isdoctype="periodical">TSL+'_'+PPN digital b-Satz+'_'+Nummer (Benennung)</processtitle>
        <!-- <processtitle isnotdoctype="multivolume">ATS+TSL+'_'+PPN digital c/a-Aufnahmel+'_'+Bandnummer</processtitle> -->
        <item isdoctype="monograph\|map" ughbinding="true" docstruct="topstruct" multiselect="false" metadata="AccessLicense">
                                  Benutzungslizenz
                                  <select label="Public Domain">public_domain</select>
                                  <select label="CC BY">cc_by</select>
                                  <select label="§ 51 VGG">51_vgg</select>
                               </item>
        <item isdoctype="periodical\|multivolume\|Series" ughbinding="true" docstruct="firstchild" multiselect="false" metadata="AccessLicense">
                                  Benutzungslizenz
                                  <select label="Public Domain">public_domain</select>
                                  <select label="CC BY">cc_by</select>
                                  <select label="§ 51 VGG">51_vgg</select>
                               </item>
        <hide>images</hide>
      </itemlist>
      <opac use="true">
        <catalogue>K10Plus</catalogue>
      </opac>
      <templates use="true"/>
      <defaultdoctype>monograph</defaultdoctype>
      <metadatageneration use="true"/>
    </createNewProcess>
    <tifheader>
      <monograph>'|[[DOC_TYPE]]'+$Doctype+'|[[HAUPTTITEL]]'+Titel+'|[[AUTOREN/HERAUSGEBER]]'+Autoren+'|[[JAHR]]'+Erscheinungsjahr+'|[[ERSCHEINUNGSORT]]'+Erscheinungsort+'|[[VERZ_STRCT]]'+ATS+'_'+PPN digital a-Satz+'|'</monograph>
      <map>'|[[DOC_TYPE]]'+$Doctype+'|[[HAUPTTITEL]]'+Titel+'|[[AUTOREN/HERAUSGEBER]]'+Autoren+'|[[JAHR]]'+Erscheinungsjahr+'|[[ERSCHEINUNGSORT]]'+Erscheinungsort+'|[[VERZ_STRCT]]'+ATS+'_'+PPN digital a-Satz+'|'</map>
      <multivolume>'|[[DOC_TYPE]]'+$Doctype+'|[[HAUPTTITEL]]'+Titel+'|[[AUTOREN/HERAUSGEBER]]'+Autoren+'|[[JAHR]]'+Erscheinungsjahr+'|[[ERSCHEINUNGSORT]]'+Erscheinungsort+'|[[VERZ_STRCT]]'+ATS+'_'+PPN digital f-Satz+'_'+Nummer (Benennung)+'|'</multivolume>
      <periodical>'|[[DOC_TYPE]]'+$Doctype+'|[[HAUPTTITEL]]'+Titel+'|[[AUTOREN/HERAUSGEBER]]'+Autoren+'|[[JAHR]]'+Erscheinungsjahr+'|[[ERSCHEINUNGSORT]]'+Erscheinungsort+'|[[VERZ_STRCT]]'+TSL+'_'+PPN digital b-Satz+'_'+Nummer (Benennung)+'|'</periodical>
    </tifheader>
    <dmsImport/>
