{
  "name": "goobi-workflow-template",
  "version": "1.0.0",
  "description": "New template for Goobi workflow.",
  "main": "index.js",
  "scripts": {
    "start": "npm run sass:dev",
    "css:build": "npm run sass:prod && npm run postcss:build",
    "postcss:build": "node runPostcss",
    "sass:dev": "source .sass-config && SASS_PATH=${SASS_SRC_PATH} sass --watch ${SRC_BOOTSTRAP}:${DEST_DEV_BOOTSTRAP} ${SRC_CSS_CUSTOM}:${DEST_DEV_CSS_CUSTOM} ${SRC_CSS_ACCESSIBILITY}:${DEST_DEV_CSS_ACCESSIBILITY}",
    "sass:prod": "source .sass-config && SASS_PATH=${SASS_SRC_PATH} sass uii/templatePG/css/src/:uii/templatePG/css/dist"
  },
  "repository": {
    "type": "git",
    "url": "https://gitea.intranda.com/marc/goobi-workflow-template.git"
  },
  "author": "intranda GmbH",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "10.4.4",
    "cssnano": "5.1.7",
    "grunt": "^1.5.2",
    "grunt-contrib-concat": "*",
    "grunt-contrib-less": "*",
    "grunt-contrib-watch": "*",
    "grunt-riot": "*",
    "grunt-sync": "*",
    "less-plugin-autoprefix": "*",
    "postcss": "8.4.12",
    "postcss-cli": "9.1.0",
    "sass": "1.49.11"
  },
  "dependencies": {
    "bootstrap": "5.1.3"
  }
}
