	<!-- ## originInfo (Original) ## -->

	<!-- Datum von (codiert) -->
	<MetadataType>
		<Name>originInfo_dateCreated_start</Name>
		<language name="en">Date created (start, ISO-8601)</language>
		<validationExpression>^(\d{4}(-\d{2}){0,2}){0,1}$</validationExpression>
	</MetadataType>
	
	<!-- Datum bis (codiert) -->
	<MetadataType>
		<Name>originInfo_dateCreated_end</Name>
		<language name="en">Date created (end, ISO-8601)</language>
		<validationExpression>^(\d{4}(-\d{2}){0,2}){0,1}$</validationExpression>
	</MetadataType>
	
	<!-- Datum (Text) -->
	<MetadataType>
		<Name>originInfo_dateCreated_text</Name>
		<language name="en">Date created (text)</language>
	</MetadataType>
	

	<!-- ## originInfo (Digitalisat) ## -->
	
	<!-- Digitalisat: Erscheinungsort -->
	<MetadataType>
		<Name>originInfo_electronic_place_placeTerm</Name>
		<language name="en">Place of publication (digital form)</language>
	</MetadataType>
	
	<!-- Digitalisat: Verlagsname -->
	<MetadataType>
		<Name>originInfo_electronic_publisher</Name>
		<language name="en">Publisher (digital form)</language>
	</MetadataType>
	
	<!-- Digitalisat: Jahr (codiert) -->
	<MetadataType>
		<Name>originInfo_electronic_dateCaptured_start</Name>
		<language name="en">Date created (digital form)</language>
		<validationExpression>^20[123]\d{1}$</validationExpression>
	</MetadataType>
	
	<!-- Digitalisat: Ausgabevermerk -->
	<MetadataType>
		<Name>originInfo_electronic_edition</Name>
